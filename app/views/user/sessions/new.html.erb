<h3>ログインして下さい。</h3>
<h1><%= @title %></h1>

<%= flash_messages %>

<%= form_for @form, url: :user_session do |f| %>
  <div>
    <%= f.label :nickname, 'ニックネーム' %>
    <%= f.text_field :nickname %>
  </div>
  <div>
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'ログイン' %>
  </div>
<% end %>
